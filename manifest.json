{
  "name": "Unhide",
  "version": "0.2",
  "description": "Shows what is hidden",
  "content_scripts": [
    {
      "matches": [
        "https://www.biznesradar.pl/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "/images/search-16.png",
      "32": "/images/search-32.png",
      "48": "/images/search-64.png",
      "128": "/images/search-128.png"
    }
  },
  "manifest_version": 2
}
